{
  "name": "banana-comic",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "bunx drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "bunx drizzle-kit migrate --config=drizzle.config.ts",
    "db:push": "bunx drizzle-kit push --config=drizzle.config.ts",
    "db:studio": "bunx drizzle-kit studio --config=drizzle.config.ts",
    "emails:dev": "bunx --bun email dev -d ./src/emails",
    "emails:export": "bunx --bun email export -d ./src/emails -o ./emails-dist"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.24",
    "@neondatabase/serverless": "^1.0.1",
    "@react-email/components": "^0.5.2",
    "ai": "^5.0.34",
    "drizzle-orm": "^0.44.5",
    "motion": "^12.23.12",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "resend": "^6.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.2",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "react-email": "4.2.8",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
